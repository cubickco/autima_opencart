                          <li><a href="{{ checkout }}" title="{{ text_checkout }}">
                              <span>{{ text_checkout }}</span></a></li>
                          {# {% endif %} #}
                      
                    </ul>
                  </li>
                  <li class="language">
                    {{ language }}
                  </li>
                  <li class="currency">
                    {{ currency }}
